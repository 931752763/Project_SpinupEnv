<mujoco model="swimmer">
    <compiler angle="degree" coordinate="local" inertiafromgeom="true"/>
    <option collision="predefined" density="1000" integrator="RK4" timestep="0.005" viscosity="0.1"/>
    <default>
        <geom conaffinity="1" condim="1" contype="1" material="geom" rgba="0.8 0.6 .4 1"/>
        <joint armature='0.1'/>
        <default class="mid">
            <site pos="-0.5 0 0" size="0.12"/>
        </default>
    </default>
    <asset>
        <texture builtin="gradient" height="100" rgb1="1 1 1" rgb2="0 0 0" type="skybox" width="100"/>
        <texture builtin="flat" height="1278" mark="cross" markrgb="1 1 1" name="texgeom" random="0.01"
                 rgb1="0.8 0.6 0.4" rgb2="0.8 0.6 0.4" type="cube" width="127"/>
        <texture builtin="checker" height="100" name="texplane" rgb1="0 0 0" rgb2="0.8 0.8 0.8" type="2d" width="100"/>
        <material name="MatPlane" reflectance="0.5" shininess="1" specular="1" texrepeat="30 30" texture="texplane"/>
        <material name="geom" texture="texgeom" texuniform="true"/>
    </asset>
    <worldbody>
        <light cutoff="100" diffuse="1 1 1" dir="-0 0 -1.3" directional="true" exponent="1" pos="0 0 1.3"
               specular=".1 .1 .1"/>
        <geom conaffinity="1" condim="3" material="MatPlane" name="floor" pos="0 0 -0.1" rgba="0.8 0.9 0.8 1"
              size="40 40 0.1" type="plane"/>
        <!--  ================= SWIMMER ================= /-->
        <body name="torso" pos="0 0 0">
            <camera name="track" mode="trackcom" pos="0 -3 3" xyaxes="1 0 0 0 1 1"/>
            <geom density="1000" fromto="0 0 0 -1 0 0" size="0.1" type="capsule"/>
            <joint axis="1 0 0" name="slider1" pos="0 0 0" type="slide"/>
            <joint axis="0 1 0" name="slider2" pos="0 0 0" type="slide"/>
            <joint axis="0 0 1" name="rot" pos="0 0 0" type="hinge"/>
            <site name="head" class="mid"/>
            <body name="body1" pos="-1 0 0">
                <geom density="1000" fromto="0 0 0 -1 0 0" size=".1" type="capsule"/>
                <joint axis="0 0 1" limited="true" name="rot1" pos="0 0 0" range="-100 100" type="hinge"/>
                <site name="site1" class="mid"/>
                <body name="body2" pos="-1 0 0">
                    <geom density="1000" fromto="0 0 0 -1 0 0" size="0.1" type="capsule"/>
                    <joint axis="0 0 1" limited="true" name="rot2" pos="0 0 0" range="-100 100" type="hinge"/>
                    <site name="site2" class="mid"/>
                    <body name="body3" pos="-1 0 0">
                        <geom density="1000" fromto="0 0 0 -1 0 0" size=".1" type="capsule"/>
                        <joint axis="0 0 1" limited="true" name="rot3" pos="0 0 0" range="-100 100" type="hinge"/>
                        <site name="site3" class="mid"/>
                        <body name="body4" pos="-1 0 0">
                            <geom density="1000" fromto="0 0 0 -1 0 0" size="0.1" type="capsule"/>
                            <joint axis="0 0 1" limited="true" name="rot4" pos="0 0 0" range="-100 100" type="hinge"/>
                            <site name="site4" class="mid"/>
                            <body name="body5" pos="-1 0 0">
                                <geom density="1000" fromto="0 0 0 -1 0 0" size="0.1" type="capsule"/>
                                <joint axis="0 0 1" limited="true" name="rot5" pos="0 0 0" range="-100 100"
                                       type="hinge"/>
                                <site name="site5" class="mid"/>
                                <body name="body6" pos="-1 0 0">
                                    <geom density="1000" fromto="0 0 0 -1 0 0" size="0.1" type="capsule"/>
                                    <joint axis="0 0 1" limited="true" name="rot6" pos="0 0 0" range="-100 100"
                                           type="hinge"/>
                                    <site name="site6" class="mid"/>
                                </body>
                            </body>
                        </body>
                    </body>
                </body>
            </body>
        </body>
    </worldbody>

    <sensor>
<!--        <gyro name="head_gyro" site="head"/>-->
        <gyro name="gyro1" site="site1"/>
        <gyro name="gyro2" site="site2"/>
        <gyro name="gyro3" site="site3"/>
        <gyro name="gyro4" site="site4"/>
        <gyro name="gyro5" site="site5"/>
        <gyro name="gyro6" site="site6"/>

        <framepos objtype="site" objname="head"/>
        <framepos objtype="site" objname="site1"/>
        <framepos objtype="site" objname="site2"/>
        <framepos objtype="site" objname="site3"/>
        <framepos objtype="site" objname="site4"/>
        <framepos objtype="site" objname="site5"/>
        <framepos objtype="site" objname="site6"/>

<!--        <torque name="joint1" site="site1"/>-->
<!--        <torque name="joint2" site="site2"/>-->
<!--        <torque name="joint3" site="site3"/>-->
<!--        <torque name="joint4" site="site4"/>-->
<!--        <torque name="joint5" site="site5"/>-->
<!--        <torque name="joint6" site="site6"/>-->

    </sensor>

    <actuator>
        <position ctrllimited="true" ctrlrange="-1 1" forcelimited="true" forcerange="-100 100" kp="100" gear="1" joint="rot1"/>
        <position ctrllimited="true" ctrlrange="-1 1" forcelimited="true" forcerange="-100 100" kp="100" gear="1" joint="rot2"/>
        <position ctrllimited="true" ctrlrange="-1 1" forcelimited="true" forcerange="-100 100" kp="100" gear="1" joint="rot3"/>
        <position ctrllimited="true" ctrlrange="-1 1" forcelimited="true" forcerange="-100 100" kp="100" gear="1" joint="rot4"/>
        <position ctrllimited="true" ctrlrange="-1 1" forcelimited="true" forcerange="-100 100" kp="100" gear="1" joint="rot5"/>
        <position ctrllimited="true" ctrlrange="-1 1" forcelimited="true" forcerange="-100 100" kp="100" gear="1" joint="rot6"/>
<!--        <motor ctrllimited="true" ctrlrange="-1 1" forcelimited="true" forcerange="-100 100" gear="150.0" joint="rot1"/>-->
<!--        <motor ctrllimited="true" ctrlrange="-1 1" forcelimited="true" forcerange="-100 100" gear="150.0" joint="rot2"/>-->
<!--        <motor ctrllimited="true" ctrlrange="-1 1" forcelimited="true" forcerange="-100 100" gear="150.0" joint="rot3"/>-->
<!--        <motor ctrllimited="true" ctrlrange="-1 1" forcelimited="true" forcerange="-100 100" gear="150.0" joint="rot4"/>-->
<!--        <motor ctrllimited="true" ctrlrange="-1 1" forcelimited="true" forcerange="-100 100" gear="150.0" joint="rot5"/>-->
<!--        <motor ctrllimited="true" ctrlrange="-1 1" forcelimited="true" forcerange="-100 100" gear="150.0" joint="rot6"/>-->
    </actuator>
</mujoco>
